<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/startGameFragment">

    <fragment
        android:id="@+id/startGameFragment"
        android:name="kktyu.xyz.insidergame.startgame.StartGameFragment"
        android:label="fragment_start_game"
        tools:layout="@layout/fragment_start_game">
        <action
            android:id="@+id/action_startGameFragment_to_selectNumberFragment"
            app:destination="@id/selectNumberFragment" />
    </fragment>
    <fragment
        android:id="@+id/selectNumberFragment"
        android:name="kktyu.xyz.insidergame.selectnumber.SelectNumberFragment"
        android:label="fragment_select_number"
        tools:layout="@layout/fragment_select_number">
        <action
            android:id="@+id/action_selectNumberFragment_to_displayConfirmationFragment"
            app:destination="@id/displayConfirmationFragment"
            app:popUpTo="@id/startGameFragment" />
    </fragment>
    <fragment
        android:id="@+id/displayConfirmationFragment"
        android:name="kktyu.xyz.insidergame.displaycomfirmation.DisplayConfirmationFragment"
        android:label="fragment_display_confirmation"
        tools:layout="@layout/fragment_display_confirmation">
        <action
            android:id="@+id/action_displayConfirmationFragment_to_roleConfirmationFragment"
            app:destination="@id/roleConfirmationFragment"
            app:popUpTo="@id/startGameFragment" />
    </fragment>
    <fragment
        android:id="@+id/roleConfirmationFragment"
        android:name="kktyu.xyz.insidergame.roleconfirmation.RoleConfirmationFragment"
        android:label="fragment_role_confirmation"
        tools:layout="@layout/fragment_role_confirmation">
        <action
            android:id="@+id/action_roleConfirmationFragment_to_themeConfirmationFragment"
            app:destination="@id/themeConfirmationFragment"
            app:popUpTo="@id/startGameFragment" />
        <action
            android:id="@+id/action_roleConfirmationFragment_to_masterConfirmationFragment"
            app:destination="@id/masterConfirmationFragment"
            app:popUpTo="@id/startGameFragment" />
        <action
            android:id="@+id/action_roleConfirmationFragment_to_displayConfirmationFragment"
            app:destination="@id/displayConfirmationFragment"
            app:popUpTo="@id/startGameFragment" />
    </fragment>
    <fragment
        android:id="@+id/masterConfirmationFragment"
        android:name="kktyu.xyz.insidergame.masterconfirmation.MasterConfirmationFragment"
        android:label="fragment_master_confirmation"
        tools:layout="@layout/fragment_master_confirmation">
        <action
            android:id="@+id/action_masterConfirmationFragment_to_questionTimeFragment"
            app:destination="@id/questionTimeFragment"
            app:popUpTo="@id/startGameFragment" />
    </fragment>
    <fragment
        android:id="@+id/themeConfirmationFragment"
        android:name="kktyu.xyz.insidergame.themeconfirmation.ThemeConfirmationFragment"
        android:label="fragment_theme_confirmation"
        tools:layout="@layout/fragment_theme_confirmation">
        <action
            android:id="@+id/action_themeConfirmationFragment_to_masterConfirmationFragment"
            app:destination="@id/masterConfirmationFragment"
            app:popUpTo="@id/startGameFragment" />
        <action
            android:id="@+id/action_themeConfirmationFragment_to_displayConfirmationFragment"
            app:destination="@id/displayConfirmationFragment"
            app:popUpTo="@id/startGameFragment" />
    </fragment>
    <fragment
        android:id="@+id/questionTimeFragment"
        android:name="kktyu.xyz.insidergame.questiontiome.QuestionTimeFragment"
        android:label="fragment_count_down"
        tools:layout="@layout/fragment_count_down">
        <action
            android:id="@+id/action_questionTimeFragment_to_discussionTimeFragment"
            app:destination="@id/discussionTimeFragment"
            app:popUpTo="@id/startGameFragment" />
    </fragment>
    <fragment
        android:id="@+id/discussionTimeFragment"
        android:name="kktyu.xyz.insidergame.discusstime.DiscussionTimeFragment"
        android:label="DiscussionTimeFragment" />
</navigation>